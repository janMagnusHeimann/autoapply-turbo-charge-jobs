  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Execution History Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze execution history\n",
    "execution_history = executor.get_execution_history()\n",
    "\n",
    "if execution_history:\n",
    "    # Convert to DataFrame for analysis\n",
    "    history_df = pd.DataFrame(execution_history)\n",
    "    \n",
    "    print(f\"Execution History Analysis ({len(history_df)} records):\")\n",
    "    \n",
    "    # Success rate by tool\n",
    "    success_by_tool = history_df.groupby('tool_name')['success'].agg(['count', 'sum', 'mean'])\n",
    "    success_by_tool.columns = ['total_executions', 'successful_executions', 'success_rate']\n",
    "    success_by_tool['success_rate'] = success_by_tool['success_rate'] * 100\n",
    "    \n",
    "    print(\"\\nSuccess rates by tool:\")\n",
    "    print(success_by_tool)\n",
    "    \n",
    "    # Execution time analysis\n",
    "    print(\"\\nExecution time statistics:\")\n",
    "    time_stats = history_df['execution_time'].describe()\n",
    "    for stat, value in time_stats.items():\n",
    "        print(f\"  {stat}: {value:.4f}s\")\n",
    "    \n",
    "    # Plot execution times over time\n",
    "    plt.figure(figsize=(12, 8))\n",
    "    \n",
    "    plt.subplot(2, 2, 1)\n",
    "    plt.plot(history_df.index, history_df['execution_time'])\n",
    "    plt.title('Execution Time Over Calls')\n",
    "    plt.xlabel('Call Number')\n",
    "    plt.ylabel('Execution Time (s)')\n",
    "    \n",
    "    plt.subplot(2, 2, 2)\n",
    "    plt.hist(history_df['execution_time'], bins=10, alpha=0.7)\n",
    "    plt.title('Execution Time Distribution')\n",
    "    plt.xlabel('Execution Time (s)')\n",
    "    plt.ylabel('Frequency')\n",
    "    \n",
    "    plt.subplot(2, 2, 3)\n",
    "    success_counts = history_df['success'].value_counts()\n",
    "    plt.pie(success_counts.values, labels=['Success', 'Failure'], autopct='%1.1f%%')\n",
    "    plt.title('Overall Success Rate')\n",
    "    \n",
    "    plt.subplot(2, 2, 4)\n",
    "    tool_times = history_df.groupby('tool_name')['execution_time'].mean()\n",
    "    tool_times.plot(kind='bar')\n",
    "    plt.title('Average Execution Time by Tool')\n",
    "    plt.xlabel('Tool Name')\n",
    "    plt.ylabel('Avg Execution Time (s)')\n",
    "    plt.xticks(rotation=45)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "else:\n",
    "    print(\"No execution history available\")"
   ]
  },
