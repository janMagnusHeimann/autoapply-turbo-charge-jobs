   "source": [
    "import sys\n",
    "from pathlib import Path\n",
    "import json\n",
    "import time\n",
    "from datetime import datetime\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "from typing import List, Dict, Any\n",
    "\n",
    "# Add backend to path\n",
    "sys.path.insert(0, str(Path.cwd().parent.parent / \"backend\"))\n",
    "\n",
    "from src.job_automation.core.tools import BaseTool, ToolRegistry, ToolExecutor, ToolResult, ToolParameter\n",
    "\n",
    "print(\"Tool calling environment ready!\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Custom Tool Development"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create custom tools for job automation\n",
    "\n",
    "class JobSearchTool(BaseTool):\n",
    "    \"\"\"Tool for searching job postings based on criteria.\"\"\"\n",
    "    \n",
    "    def _define_parameters(self) -> List[ToolParameter]:\n",
    "        return [\n",
    "            ToolParameter(\"keywords\", \"string\", \"Job keywords to search for\", required=True),\n",
    "            ToolParameter(\"location\", \"string\", \"Job location\", required=False, default=\"Remote\"),\n",
    "            ToolParameter(\"experience_level\", \"string\", \"Experience level (junior/mid/senior)\", required=False, default=\"mid\"),\n",
    "            ToolParameter(\"limit\", \"integer\", \"Maximum number of results\", required=False, default=10)\n",
    "        ]\n",
    "    \n",
    "    def execute(self, **kwargs) -> ToolResult:\n",
    "        keywords = kwargs.get(\"keywords\", \"\")\n",
    "        location = kwargs.get(\"location\", \"Remote\")\n",
    "        experience_level = kwargs.get(\"experience_level\", \"mid\")\n",
    "        limit = kwargs.get(\"limit\", 10)\n",
    "        \n",
    "        # Simulate job search results\n",
    "        mock_jobs = [\n",
    "            {\n",
    "                \"title\": f\"Senior {keywords} Developer\",\n",
    "                \"company\": \"TechCorp\",\n",
    "                \"location\": location,\n",
    "                \"salary_range\": \"$120,000 - $160,000\",\n",
    "                \"posted_date\": \"2025-01-18\"\n",
    "            },\n",
    "            {\n",
    "                \"title\": f\"{keywords} Engineer\",\n",
    "                \"company\": \"StartupInc\",\n",
    "                \"location\": location,\n",
    "                \"salary_range\": \"$100,000 - $140,000\",\n",
    "                \"posted_date\": \"2025-01-17\"\n",
    "            },\n",
    "            {\n",
    "                \"title\": f\"Lead {keywords} Architect\",\n",
    "                \"company\": \"BigTech\",\n",
    "                \"location\": location,\n",
    "                \"salary_range\": \"$150,000 - $200,000\",\n",
    "                \"posted_date\": \"2025-01-16\"\n",
    "            }\n",
    "        ]\n",
    "        \n",
    "        # Filter by limit\n",
    "        results = mock_jobs[:limit]\n",
    "        \n",
    "        return ToolResult(\n",
    "            success=True,\n",
    "            data={\n",
    "                \"jobs\": results,\n",
    "                \"total_found\": len(results),\n",
    "                \"search_criteria\": {\n",
    "                    \"keywords\": keywords,\n",
    "                    \"location\": location,\n",
    "                    \"experience_level\": experience_level\n",
    "                }\n",
    "            }\n",
    "        )\n",
    "\n",
    "\n",
    "class CompanyResearchTool(BaseTool):\n",
    "    \"\"\"Tool for researching company information.\"\"\"\n",
    "    \n",
    "    def _define_parameters(self) -> List[ToolParameter]:\n",
    "        return [\n",
    "            ToolParameter(\"company_name\", \"string\", \"Name of the company to research\", required=True),\n",
    "            ToolParameter(\"include_financials\", \"boolean\", \"Include financial information\", required=False, default=False)\n",
    "        ]\n",
    "    \n",
    "    def execute(self, **kwargs) -> ToolResult:\n",
    "        company_name = kwargs.get(\"company_name\", \"\")\n",
    "        include_financials = kwargs.get(\"include_financials\", False)\n",
    "        \n",
    "        if not company_name:\n",
    "            return ToolResult(success=False, error=\"Company name is required\")\n",
    "        \n",
    "        # Simulate company research\n",
    "        company_info = {\n",
    "            \"name\": company_name,\n",
    "            \"industry\": \"Technology\",\n",
    "            \"size\": \"1000-5000 employees\",\n",
    "            \"culture\": \"Innovation-focused, collaborative\",\n",
    "            \"benefits\": [\"Health insurance\", \"401k matching\", \"Flexible PTO\", \"Remote work options\"],\n",
    "            \"recent_news\": [\n",
    "                \"Announced new product line\",\n",
    "                \"Expanded to European market\",\n",
    "                \"Received Series B funding\"\n",
    "            ],\n",
    "            \"glassdoor_rating\": 4.2,\n",
    "            \"headquarters\": \"San Francisco, CA\"\n",
    "        }\n",
    "        \n",
    "        if include_financials:\n",
    "            company_info[\"financials\"] = {\n",
    "                \"revenue\": \"$500M ARR\",\n",
    "                \"funding_stage\": \"Series B\",\n",
    "                \"valuation\": \"$2B\",\n",
    "                \"growth_rate\": \"40% YoY\"\n",
    "            }\n",
    "        \n",
    "        return ToolResult(success=True, data=company_info)\n",
    "\n",
    "\n",
    "class CoverLetterGeneratorTool(BaseTool):\n",
    "    \"\"\"Tool for generating personalized cover letters.\"\"\"\n",
    "    \n",
    "    def _define_parameters(self) -> List[ToolParameter]:\n",
    "        return [\n",
    "            ToolParameter(\"job_title\", \"string\", \"Title of the job position\", required=True),\n",
    "            ToolParameter(\"company_name\", \"string\", \"Name of the company\", required=True),\n",
    "            ToolParameter(\"user_experience\", \"string\", \"User's relevant experience\", required=True),\n",
    "            ToolParameter(\"job_requirements\", \"string\", \"Key job requirements\", required=False, default=\"\"),\n",
    "            ToolParameter(\"tone\", \"string\", \"Tone of the letter (professional/enthusiastic/casual)\", required=False, default=\"professional\")\n",
    "        ]\n",
    "    \n",
    "    def execute(self, **kwargs) -> ToolResult:\n",
    "        job_title = kwargs.get(\"job_title\", \"\")\n",
    "        company_name = kwargs.get(\"company_name\", \"\")\n",
    "        user_experience = kwargs.get(\"user_experience\", \"\")\n",
    "        job_requirements = kwargs.get(\"job_requirements\", \"\")\n",
    "        tone = kwargs.get(\"tone\", \"professional\")\n",
    "        \n",
    "        if not all([job_title, company_name, user_experience]):\n",
    "            return ToolResult(success=False, error=\"Job title, company name, and user experience are required\")\n",
    "        \n",
    "        # Generate cover letter template\n",
    "        cover_letter = f\"\"\"Dear Hiring Manager,\n",
    "\n",
    "I am writing to express my strong interest in the {job_title} position at {company_name}. With my background in {user_experience}, I am excited about the opportunity to contribute to your team.\n",
    "\n",
    "My experience includes {user_experience}, which directly aligns with your needs for this role. {f'Specifically, I have expertise in {job_requirements}' if job_requirements else ''}\n",
    "\n",
    "I am particularly drawn to {company_name} because of your commitment to innovation and excellence in the technology space. I would welcome the opportunity to discuss how my skills and passion can contribute to your continued success.\n",
    "\n",
    "Thank you for considering my application. I look forward to hearing from you.\n",
    "\n",
    "Best regards,\n",
    "[Your Name]\"\"\"\n",
    "        \n",
    "        return ToolResult(\n",
    "            success=True,\n",
    "            data={\n",
    "                \"cover_letter\": cover_letter,\n",
    "                \"word_count\": len(cover_letter.split()),\n",
    "                \"tone\": tone,\n",
    "                \"personalization_score\": 0.8  # Mock score\n",
    "            }\n",
    "        )\n",
    "\n",
    "print(\"Custom tools defined successfully!\")"
   ]
  },
