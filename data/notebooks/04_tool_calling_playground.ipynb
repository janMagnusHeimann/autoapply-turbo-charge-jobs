   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Performance Testing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Performance testing - execute multiple tools\n",
    "print(\"Performance Testing:\")\n",
    "\n",
    "# Test multiple job searches\n",
    "search_keywords = [\"Python\", \"JavaScript\", \"Java\", \"React\", \"Node.js\"]\n",
    "execution_times = []\n",
    "\n",
    "for keyword in search_keywords:\n",
    "    start_time = time.time()\n",
    "    \n",
    "    result = executor.execute_tool(\n",
    "        \"JobSearchTool\",\n",
    "        keywords=keyword,\n",
    "        location=\"Remote\",\n",
    "        limit=10\n",
    "    )\n",
    "    \n",
    "    execution_time = time.time() - start_time\n",
    "    execution_times.append(execution_time)\n",
    "    \n",
    "    print(f\"  {keyword}: {execution_time:.4f}s - {'Success' if result.success else 'Failed'}\")\n",
    "\n",
    "# Calculate statistics\n",
    "avg_time = sum(execution_times) / len(execution_times)\n",
    "max_time = max(execution_times)\n",
    "min_time = min(execution_times)\n",
    "\n",
    "print(f\"\\nPerformance Statistics:\")\n",
    "print(f\"  Average execution time: {avg_time:.4f}s\")\n",
    "print(f\"  Max execution time: {max_time:.4f}s\")\n",
    "print(f\"  Min execution time: {min_time:.4f}s\")\n",
    "\n",
    "# Plot performance\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.bar(search_keywords, execution_times)\n",
    "plt.title('Tool Execution Performance by Keyword')\n",
    "plt.xlabel('Search Keyword')\n",
    "plt.ylabel('Execution Time (seconds)')\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
