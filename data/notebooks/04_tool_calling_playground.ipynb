   "source": [
    "## Tool Registry and Management"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create tool registry and register tools\n",
    "registry = ToolRegistry()\n",
    "\n",
    "# Register our custom tools\n",
    "registry.register_tool(JobSearchTool)\n",
    "registry.register_tool(CompanyResearchTool)\n",
    "registry.register_tool(CoverLetterGeneratorTool)\n",
    "\n",
    "print(\"Registered tools:\")\n",
    "for tool_name in registry.list_tool_names():\n",
    "    print(f\"  - {tool_name}\")\n",
    "\n",
    "# Get tool information\n",
    "print(\"\\nTool Details:\")\n",
    "for tool_name in registry.list_tool_names():\n",
    "    tool_info = registry.get_tool_info(tool_name)\n",
    "    print(f\"\\n{tool_name}:\")\n",
    "    print(f\"  Description: {tool_info['description']}\")\n",
    "    print(f\"  Parameters: {len(tool_info['parameters'])}\")\n",
    "    for param in tool_info['parameters']:\n",
    "        required = \"(required)\" if param['required'] else \"(optional)\"\n",
    "        print(f\"    - {param['name']}: {param['type']} {required}\")"
   ]
  },
