   "source": [
    "# Get tool usage statistics\n",
    "usage_stats = executor.get_tool_usage_stats()\n",
    "\n",
    "print(\"Tool Usage Statistics:\")\n",
    "print(f\"  Total executions: {usage_stats['total_executions']}\")\n",
    "print(f\"  Successful executions: {usage_stats['successful_executions']}\")\n",
    "print(f\"  Failed executions: {usage_stats['failed_executions']}\")\n",
    "print(f\"  Success rate: {(usage_stats['successful_executions'] / usage_stats['total_executions'] * 100):.1f}%\")\n",
    "print(f\"  Average execution time: {usage_stats['average_execution_time']:.4f}s\")\n",
    "\n",
    "print(\"\\nTool usage frequency:\")\n",
    "for tool_name, count in usage_stats['tool_usage_frequency'].items():\n",
    "    print(f\"  {tool_name}: {count} executions\")\n",
    "\n",
    "# Visualize tool usage\n",
    "if usage_stats['tool_usage_frequency']:\n",
    "    tools = list(usage_stats['tool_usage_frequency'].keys())\n",
    "    counts = list(usage_stats['tool_usage_frequency'].values())\n",
    "    \n",
    "    plt.figure(figsize=(10, 6))\n",
    "    plt.pie(counts, labels=tools, autopct='%1.1f%%')\n",
    "    plt.title('Tool Usage Distribution')\n",
    "    plt.show()"
   ]
