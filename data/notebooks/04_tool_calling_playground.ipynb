    "## Save Experiment Results"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Compile and save experiment results\n",
    "experiment_results = {\n",
    "    \"experiment_date\": datetime.now().isoformat(),\n",
    "    \"tools_registered\": len(registry.list_tool_names()),\n",
    "    \"tools_tested\": registry.list_tool_names(),\n",
    "    \"usage_statistics\": usage_stats,\n",
    "    \"performance_metrics\": {\n",
    "        \"avg_execution_time\": avg_time,\n",
    "        \"max_execution_time\": max_time,\n",
    "        \"min_execution_time\": min_time,\n",
    "        \"tested_keywords\": search_keywords\n",
    "    }\n",
    "}\n",
    "\n",
    "# Add execution history statistics if available\n",
    "if execution_history:\n",
    "    experiment_results[\"execution_history_stats\"] = {\n",
    "        \"total_executions\": len(execution_history),\n",
    "        \"time_statistics\": time_stats.to_dict() if 'time_stats' in locals() else {},\n",
    "        \"success_by_tool\": success_by_tool.to_dict() if 'success_by_tool' in locals() else {}\n",
    "    }\n",
    "\n",
    "# Save results\n",
    "output_dir = Path.cwd().parent / \"experiments\"\n",
    "output_dir.mkdir(exist_ok=True)\n",
    "\n",
    "with open(output_dir / \"tool_calling_experiment.json\", \"w\") as f:\n",
    "    json.dump(experiment_results, f, indent=2)\n",
    "\n",
    "print(f\"Experiment results saved to {output_dir / 'tool_calling_experiment.json'}\")\n",
    "\n",
    "# Save tool schemas for documentation\n",
    "tool_schemas = {}\n",
    "for tool_name in registry.list_tool_names():\n",
